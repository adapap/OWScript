{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "OWScript",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.owscript",
          "match": "(?i)\\b(#import|if|while|for|return)\\b"
        },
        {
          "name": "keyword.workshop.owscript",
          "match": "(?i)\\b(RULE|EVENT|CONDITIONS|ACTIONS)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.owscript",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.owscript",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.owscript"
}